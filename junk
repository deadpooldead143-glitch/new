from web3 import Web3

# ---------- CONFIG ----------
PRIVATE_KEY = "0xe8f2a825af4b76820675289a9794d0ecd5f95bc1a8e7dbe70742bb76fdf7699b"   # replace
FROM_ADDRESS = "0xCDfEe9DB86c45243d23FA21aEaf1459A1fCC9371"
TO_ADDRESS = "0xAD179DBBA1FAc6c839C6827d072672250BCBc991"
RPC_URL = "https://mainnet.infura.io/v3/df7350538a074b36874c4da7f4efea00"  # replace with your provider

# Your transaction data (from JSON)
DATA = ""

# ---------- SETUP ----------
w3 = Web3(Web3.HTTPProvider(RPC_URL))
if not w3.is_connected():
    raise Exception("Could not connect to Ethereum RPC")

# Always fetch the latest nonce for the sender
nonce = w3.eth.get_transaction_count(FROM_ADDRESS, "pending")

# Suggest fees
base_fee = w3.eth.gas_price
max_priority_fee = w3.to_wei("2", "gwei")
max_fee = base_fee + max_priority_fee

# Build transaction
tx = {
    "chainId": 1,
    "from": FROM_ADDRESS,
    "to": TO_ADDRESS,
    "data": DATA,
    "nonce": nonce,
    "maxFeePerGas": max_fee,
    "maxPriorityFeePerGas": max_priority_fee
}

# Estimate gas safely
gas_estimate = w3.eth.estimate_gas({
    "from": FROM_ADDRESS,
    "to": TO_ADDRESS,
    "data": DATA
})
tx["gas"] = int(gas_estimate * 1.2)  # add buffer

# Sign + send
signed_tx = w3.eth.account.sign_transaction(tx, PRIVATE_KEY)
tx_hash = w3.eth.send_raw_transaction(signed_tx.raw_transaction)

print("âœ… Transaction sent! Hash:", w3.to_hex(tx_hash))
